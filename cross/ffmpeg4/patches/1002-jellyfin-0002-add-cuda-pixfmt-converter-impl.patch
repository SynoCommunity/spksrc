-    const float A = param == SCALE_CUDA_PARAM_DEFAULT ? 0.0f : -param;
-
-    float4 res;
-    res.x = ((A * (x + 1) - 5 * A) * (x + 1) + 8 * A) * (x + 1) - 4 * A;
-    res.y = ((A + 2) * x - (A + 3)) * x * x + 1;
-    res.z = ((A + 2) * (1 - x) - (A + 3)) * (1 - x) * (1 - x) + 1;
-    res.w = 1.0f - res.x - res.y - res.z;
-
-    return res;
-}
