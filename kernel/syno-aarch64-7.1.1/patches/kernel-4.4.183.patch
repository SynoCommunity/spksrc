--- arch/arm/boot/dts/imx6sl.dtsi
+++ arch/arm/boot/dts/imx6sl.dtsi
@@ -657,7 +657,7 @@ sdma: sdma@020ec000 {
 				reg = <0x020ec000 0x4000>;
 				interrupts = <0 2 IRQ_TYPE_LEVEL_HIGH>;
 				clocks = <&clks IMX6SL_CLK_SDMA>,
