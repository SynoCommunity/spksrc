diff --git drivers/net/ppp/pppox.c drivers/net/ppp/pppox.c
index 0e1b30622477..011fbd10cb73 100644
--- drivers/net/ppp/pppox.c
+++ drivers/net/ppp/pppox.c
@@ -22,6 +22,7 @@
 #include <linux/string.h>
